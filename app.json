{
  "expo": {
    "name": "Shark-Music-Player",
    "slug": "Shark-Music-Player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sharkmusic.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/sharkmusic.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sharkmusic.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.example.sharkmusicplayer"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Shark Music Player to access your photos.",
          "savePhotosPermission": "Allow Shark Music Player to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
